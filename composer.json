{
    "name": "gradosevic/laravel-export",
    "description": "Create a static site bundle from a Laravel app",
    "keywords": [
        "spatie",
        "gradosevic",
        "laravel-export"
    ],
    "homepage": "https://github.com/spatie/laravel-export",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/console": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/contracts": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "nyholm/psr7": "^1.1",
        "psr/http-message": "^1.0",
        "symfony/console": "^3.0|^4.2|^5.0",
        "symfony/dom-crawler": "^5.2",
        "symfony/http-foundation": "^4.2|^5.0",
        "symfony/process": "^5.0",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Export\\ExportServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Export\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Export\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
